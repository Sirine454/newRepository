ALGORITHM array
VAR 
i,j:INTEGER;
a1 : ARRAY_OF INTEGER[4];
a2 : ARRAY_OF INTEGER[5];
sum:INTEGER;
//solution 1 prob1
BEGIN
a1:=[3,1,7,9];
a2:=[2,4,7,9,3];
sum:=0
FOR i FROM 0 TO length(a1) STEP 1  DO
   FOR j FROM 0 TO length(a2) STEP 2  DO 
      IF (a1[i]<>a2[j])
      THEN
      sum:= sum+a1[i]
      END_IF
END_FOR
END_FOR
FOR i FROM 0 TO length(a2) STEP 1  DO
   FOR j FROM 0 TO length(a1) STEP 2  DO 
      IF (a2[i]<>a1[j])
      THEN
      sum:=sum+a2[i]

      END_IF
END_FOR
END_FOR
Write(sum);
END

