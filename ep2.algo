BEGIN
 ALGORITHM hach
VAR
    htab : HASH_TABLE<INTEGER,INTEGER>;
    a1,a2: ARRAY_OF INTEGER[50];
    i,sum,count : INTEGER;
    sum:=0
BEGIN
    FOR i FROM 0 TO length(a1)-1 STEP 1 DO
        htab.insert(a1[i],1);
    END_FOR

    FOR i FROM 0 TO length(a2)-1 STEP 1 DO
        IF (htab.lookup(a2[i])= TRUE) THEN
            count:= htab.get(a2[i]);
            htab.insert(a2[i],count+1);
        ELSE
            htab.insert(a2[i],1);
        END_IF
    END_FOR
    // loop the hash
    FOR i FROM 0 TO length(a1)-1 STEP 1 DO
        IF ( htab.get(a1[i]) not 1) THEN
            sum := sum +a1[i];
        END_IF 
    END_FOR

    FOR i FROM 0 TO length(a2)-1 STEP 1 DO
        IF ( htab.get(a2[i]) not 1) THEN
            sum := sum + a2[i];
        END_IF 
    END_FOR
    
    sum:=sum*2;
    Write(sum);
END
